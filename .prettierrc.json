{
  "singleQuote": true,
  "printWidth": 120,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    { "files": "*.scss", "options": { "tabWidth": 4 } },
    { "files": "*.html", "options": { "tabWidth": 4 } }
  ]
}
