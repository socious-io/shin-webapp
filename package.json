{
  "name": "shin-webapp",
  "private": true,
  "version": "8.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host 0.0.0.0 --mode development",
    "preview": "vite preview",
    "test": "vite --port 3000 --host 0.0.0.0 --mode test",
    "build": "npx vite build --mode development",
    "open:android": "vite build --mode production && npx cap sync android && npx cap open android",
    "open:ios": "vite build --mode production && npx cap sync ios && npx cap open ios",
    "build:icons": "npx @capacitor/assets generate --iconBackgroundColor '#183E3B' --splashBackgroundColor '#183E3B'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:update": "cypress run --env updateSnapshots=true",
    "prettier": "npx prettier  .",
    "prettier:fix": "npx prettier --write",
    "eslint:fix": "eslint --fix",
    "typecheck": "tsc",
    "typecheck:staged": "tsc-files",
    "generate:translations": "node src/core/helpers/generate-languages.js",
    "lint": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor-firebase/messaging": "^6.1.0",
    "@capacitor/action-sheet": "^6.0.1",
    "@capacitor/android": "^6.1.1",
    "@capacitor/app": "^6.0.0",
    "@capacitor/camera": "^6.0.1",
    "@capacitor/core": "^6.1.1",
    "@capacitor/dialog": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.1.1",
    "@capacitor/preferences": "^6.0.1",
    "@capacitor/push-notifications": "^6.0.1",
    "@capacitor/toast": "^6.0.1",
    "@datadog/browser-rum": "^6.16.0",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.0",
    "@ionic/pwa-elements": "^3.3.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.0",
    "country-flag-icons": "^1.5.19",
    "dotenv": "^17.2.1",
    "firebase": "^10.12.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "iso-country-currency": "^0.7.2",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "markdown-to-jsx": "^7.7.12",
    "moment": "^2.30.1",
    "qrcode.react": "^4.2.0",
    "ramda": "^0.31.3",
    "react": "^18.3.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "react-select": "^5.10.2",
    "socket.io-client": "^4.8.1",
    "ssi-auth-lib": "^0.0.12",
    "vite-tsconfig-paths": "^5.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.2",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@swc/core": "^1.13.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/ramda": "^0.31.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "cypress": "^14.5.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^4.2.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "tsc-files": "^1.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run typecheck && npm run build"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint:fix",
      "npm run prettier:fix"
    ],
    "*.{css,scss,md}": "npm run prettier:fix",
    "src/core/translation/locales/en/**/*.json": [
      "npm run generate:translations"
    ]
  }
}
